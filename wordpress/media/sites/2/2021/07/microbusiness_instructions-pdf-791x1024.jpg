/* step list */
ol.steps {
    padding-left: 0 !important;
    margin-left: 30px !important;
}

ol.steps li {
    list-style: none;
    border-left: 5px solid #fec02f;
    font-size: 1.75rem;
    font-weight: 700;
    position: relative;
    padding-left: 45px;
    line-height: 1.5;
}

ol.steps li:before {
    content: "";
    font-family: "Public Sans";
    font-size: 1.75rem;
    font-weight: 700;
    padding: 0 13px 0 13px;
    border: 5px solid #fec02f;
    background-color: #fff;
    border-radius: 50px;
    text-align: center;
    position: absolute;
    left: -29px;
    top: -5px;
    margin-left: -1px;
}

