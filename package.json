    "@web/dev-server": "^0.1.22",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.989.0",
    "axe-core": "^4.3.3",
    "axe-playwright": "^1.1.8",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@cagov/ds-accordion": "^1.0.8",
