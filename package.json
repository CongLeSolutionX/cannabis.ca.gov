{
  "name": "cannabis.ca.gov",
  "version": "2.0.0",
  "description": "Static site generator for https://headless.cannabis.ca.gov",
  "main": "index.html",
  "scripts": {
    "deploy:redirects": "node src/js/redirects/assign.js",
    "build:dev": "cross-env NODE_ENV=development eleventy",
    "build:development": "cross-env SITE_ENV=development eleventy",
    "build:local": "cross-env SITE_ENV=localhost eleventy",
    "build:production": "cross-env SITE_ENV=production eleventy",
    "build:staging": "cross-env SITE_ENV=staging eleventy",
    "build:test_staging": "cross-env SITE_ENV=test_staging eleventy",
    "build:test": "cross-env SITE_ENV=test eleventy",
    "build": "eleventy",
    "dev": "npm run watch:dev",
    "start": "npm run watch",
    "watch:dev": "cross-env NODE_ENV=development SITE_ENV=localhost eleventy --serve --incremental",
    "watch:production": "cross-env NODE_ENV=development SITE_ENV=production eleventy --serve --incremental",
    "watch": "eleventy --serve"
  },
  "keywords": [
    "@cagov",
    "Office of Data and Innovation",
    "Department of Cannabis Control"
  ],
  "repository": "https://github.com/cannabis.ca.gov",
  "author": "Office of Data and Innovation",
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "^2.1189.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "cheerio": "^1.0.0-rc.10",
    "clean-css": "^5.3.1",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "static-content-cannabis": "git@github.com:cagov/static-content-cannabis.git"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@cagov/11ty-build-system": "^0.3.1",
    "@cagov/ds-accordion": "^2.0.3",
    "@cagov/ds-agency-footer": "^1.0.2",
    "@cagov/ds-back-to-top": "^2.0.2",
    "@cagov/ds-base-css": "^1.0.19",
    "@cagov/ds-branding": "^1.0.8",
    "@cagov/ds-button-grid": "^2.0.2",
    "@cagov/ds-dropdown-menu": "^2.0.1",
    "@cagov/ds-feature-card": "^2.0.0",
    "@cagov/ds-google-translate": "^1.0.2",
    "@cagov/ds-icons": "0.0.1",
    "@cagov/ds-link-grid": "^3.0.0",
    "@cagov/ds-link-icon": "^2.0.0",
    "@cagov/ds-page-alert": "^2.0.2",
    "@cagov/ds-page-feedback": "^1.1.3",
    "@cagov/ds-page-navigation": "^2.2.0",
    "@cagov/ds-pagination": "^2.0.2",
    "@cagov/ds-regulatory-outline": "^1.0.1",
    "@cagov/ds-site-footer": "^2.0.0",
    "@cagov/ds-site-header": "^2.0.0",
    "@cagov/ds-site-navigation": "^3.0.0",
    "@cagov/ds-skip-to-content": "^2.0.0",
    "@cagov/ds-statewide-footer": "^2.0.0",
    "@cagov/ds-statewide-header": "^2.0.0",
    "@cagov/ds-step-list": "^3.0.0",
    "@cagov/ds-table": "^1.0.1"
  }
}
